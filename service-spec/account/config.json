{
  "name": "accountservice",
  "version": "3.0.1",
  "groupId": "com.mservicetech",
  "artifactId": "account-service-seata",
  "rootPackage": "com.mservicetech.accountservice",
  "handlerPackage": "com.mservicetech.accountservic.handler",
  "modelPackage": "com.mservicetech.accountservic.model",
  "overwriteHandler": false,
  "overwriteHandlerTest": false,
  "overwriteModel": false,
  "httpPort": 8081,
  "enableHttp": false,
  "httpsPort": 8441,
  "enableHttps": true,
  "enableHttp2": true,
  "enableRegistry": false,
  "supportDb": true,
  "supportH2ForTest": true,
  "supportClient": true,
  "specChangeCodeReGenOnly": true,
  "generateValuesYml": true,
  "skipPomFile": true,
  "dockerOrganization": "mservicetech",
  "generateEnvVars": {
    "generate": true,
    "skipArray": true,
    "skipMap": true,
    "exclude": [
      "handerl.yml",
      "values.yml",
      "config.yml"
    ]
  }
}
